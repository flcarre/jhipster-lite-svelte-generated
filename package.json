{
  "name": "jhipster",
  "devDependencies": {
    "typescript": "~4.5.4",
    "ts-jest": "27.1.3",
    "tslib": "2.3.1",
    "svelte-jester": "2.3.2",
    "svelte-preprocess": "4.10.1",
    "svelte-check": "2.2.6",
    "svelte": "3.44.0",
    "prettier-plugin-svelte": "2.5.0",
    "prettier": "2.5.1",
    "jest-sonar-reporter": "2.0.0",
    "jest-transform-stub": "2.0.0",
    "jest": "27.5.1",
    "eslint-plugin-svelte3": "3.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint": "7.32.0",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "babel-jest": "27.5.1",
    "@typescript-eslint/parser": "5.10.1",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@types/jest": "27.4.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/svelte": "3.0.3",
    "@sveltejs/kit": "next",
    "@sveltejs/adapter-auto": "next",
    "@babel/preset-env": "7.16.11",
    "prettier-plugin-packagejson": "2.2.15",
    "prettier-plugin-java": "1.6.1",
    "prettier": "2.5.1",
    "lint-staged": "12.3.4",
    "husky": "7.0.4",
    "@prettier/plugin-xml": "1.2.0"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "check,watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "package": "svelte-kit package",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build": "svelte-kit build",
    "start": "svelte-kit dev --port 9000",
    "preview": "svelte-kit preview",
    "dev": "svelte-kit dev --port 9000",
    "prepare": "husky install",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\""
  },
  "dependencies": {
    "svelte-navigator": "3.1.5"
  },
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Project",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "type": "module",
  "cacheDirectories": [
    "node_modules"
  ]
}
